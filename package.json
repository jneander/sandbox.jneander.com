{
  "name": "sandbox",
  "engines": {
    "node": ">=6.9.5"
  },
  "version": "0.0.0",
  "description": "A place to exercise various projects together and dogfood their development environments",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.development.js --progress",
    "build:production": "webpack --config ./config/webpack.production.js --progress",
    "build:stats": "webpack --config ./config/webpack.development.js --profile --json",
    "build:watch": "webpack --config ./config/webpack.development.js --progress --watch",
    "clear": "rimraf dist",
    "deploy": "npm run build-production",
    "dev": "node scripts/dev-server.js",
    "nuke": "rimraf node_modules",
    "spec": "karma start config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jneander/sandbox.git"
  },
  "author": "Jeremy Neander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jneander/sandbox/issues"
  },
  "homepage": "https://github.com/jneander/sandbox#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "instructure-icons": "^4.2.0",
    "instructure-ui": "^2.5.0",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "write-file-webpack-plugin": "^4.0.2"
  },
  "dependencies": {
    "history": "^4.6.1",
    "instructure-ui": "^2.5.1",
    "normalize.css": "^7.0.0",
    "qs": "^6.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sinon": "^2.3.8",
    "url-pattern": "^1.0.3",
    "webpack": "^2.7.0"
  }
}
