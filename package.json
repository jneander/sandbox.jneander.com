{
  "name": "sandbox.jneander.com",
  "engines": {
    "node": "^10.15.3",
    "yarn": "^1.15.2"
  },
  "scripts": {
    "build": "NODE_ENV=development build-app --config ./config/app.js",
    "build:production": "NODE_ENV=production build-app --config ./config/app.js",
    "build:watch": "NODE_ENV=development build-app --config ./config/app.js --watch",
    "clean": "rimraf __build__",
    "format": "format-code",
    "lint": "lint-code",
    "lint:fix": "lint-code --fix",
    "start:watch": "NODE_ENV=development build-app --config ./config/app.js --watch --serve",
    "test": "NODE_ENV=test run-specs --config config/specs.js",
    "test:watch": "NODE_ENV=test run-specs --config config/specs.js --watch"
  },
  "version": "1.0.0",
  "description": "Projects for Figuring Things Out",
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/sandbox.jneander.com"
  },
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@instructure/ui-a11y": "^5.6.0",
    "@instructure/ui-buttons": "^5.6.0",
    "@instructure/ui-elements": "^5.6.0",
    "@instructure/ui-forms": "^5.6.0",
    "@instructure/ui-i18n": "^5.6.0",
    "@instructure/ui-icons": "^5.6.0",
    "@instructure/ui-layout": "^5.6.0",
    "@instructure/ui-menu": "^5.6.0",
    "@instructure/ui-pagination": "^5.6.0",
    "@instructure/ui-tabs": "^5.6.0",
    "@instructure/ui-themeable": "^5.6.0",
    "@instructure/ui-themes": "^5.6.0",
    "@instructure/ui-utils": "^5.6.0",
    "@jneander/data-grid": "^0.1.0",
    "normalize.css": "^8.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-measure": "^2.0.2",
    "react-scroll-sync": "^0.5.0"
  },
  "devDependencies": {
    "@jneander/dev-tools": "^0.0.13",
    "chai": "^4.1.2",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.4.1"
  }
}
